library(PhenoPro)
require(PhenoPro)
PhenoPro()
WorkingPath <- "C:/Users/Yifan/Desktop/Chen/PRO 04/PhenoProWorking"
setwd(WorkingPath)
RawData <- read.table("SDS.T.txt", header = TRUE)
R4DX <- RawData$R4DX
nwR4DX.F <- rep(0, length(R4DX))
for(i in seq(length(R4DX))){
if(R4DX[i] <= 1) {
nwR4DX.F[i] <- 'H'
} else {
nwR4DX.F[i] <- 'D'
}
}
RawData$R4DX.F <- nwR4DX.F
Result <- PhenoPro(
data = subset(RawData, LabelDeve == "R1"),
label = "R4DX.F",
defaultLabel = "D",
features = c("LIGHT", "Phi2"),
block = "RowRun",
method = "SVM",
orderby = "Time.n",
width = 5,
cvNumber = 100,
testBlockProp = 0.2,
visualization = FALSE)
